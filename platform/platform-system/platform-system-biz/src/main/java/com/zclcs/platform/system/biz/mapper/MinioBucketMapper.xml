<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zclcs.platform.system.biz.mapper.MinioBucketMapper">

    <sql id="baseSql">
        select mb.id, mb.bucket_name, mb.bucket_policy, mb.create_at, mb.update_at
        from system_minio_bucket mb
            ${ew.customSqlSegment}
    </sql>
    <select id="findPageVo" resultType="com.zclcs.platform.system.api.entity.vo.MinioBucketVo">
        <include refid="baseSql"/>
    </select>
    <select id="findListVo" resultType="com.zclcs.platform.system.api.entity.vo.MinioBucketVo">
        <include refid="baseSql"/>
    </select>
    <select id="findOneVo" resultType="com.zclcs.platform.system.api.entity.vo.MinioBucketVo">
        <include refid="baseSql"/>
    </select>
</mapper>
