<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zclcs.platform.system.mapper.GeneratorConfigMapper">

    <sql id="baseSql">
        select gc.id,
               gc.server_name,
               gc.author,
               gc.base_package,
               gc.entity_package,
               gc.ao_package,
               gc.vo_package,
               gc.mapper_package,
               gc.mapper_xml_package,
               gc.service_package,
               gc.service_impl_package,
               gc.controller_package,
               gc.is_trim,
               gc.trim_value,
               gc.exclude_columns,
               gc.create_at
        from system_generator_config gc
            ${ew.customSqlSegment}
    </sql>
    <select id="findPageVo" resultType="com.zclcs.platform.system.api.bean.vo.GeneratorConfigVo">
        <include refid="baseSql"/>
    </select>
    <select id="findListVo" resultType="com.zclcs.platform.system.api.bean.vo.GeneratorConfigVo">
        <include refid="baseSql"/>
    </select>
    <select id="findOneVo" resultType="com.zclcs.platform.system.api.bean.vo.GeneratorConfigVo">
        <include refid="baseSql"/>
    </select>

</mapper>
