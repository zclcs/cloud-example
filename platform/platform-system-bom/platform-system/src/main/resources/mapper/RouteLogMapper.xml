<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zclcs.platform.system.mapper.RouteLogMapper">

    <sql id="baseSql">
        select srl.route_id,
               srl.route_ip,
               srl.request_uri,
               srl.target_uri,
               srl.request_method,
               srl.request_time,
               srl.target_server,
               srl.location,
               srl.code,
               srl.time,
               srl.create_at,
               srl.update_at
        from system_route_log srl
            ${ew.customSqlSegment}
    </sql>
    <sql id="baseCountSql">
        select count(*)
        from system_route_log srl
            ${ew.customSqlSegment}
    </sql>
    <select id="findPageVo" resultType="com.zclcs.platform.system.api.bean.vo.RouteLogVo">
        <include refid="baseSql"/>
    </select>
    <select id="findListVo" resultType="com.zclcs.platform.system.api.bean.vo.RouteLogVo">
        <include refid="baseSql"/>
    </select>
    <select id="findOneVo" resultType="com.zclcs.platform.system.api.bean.vo.RouteLogVo">
        <include refid="baseSql"/>
    </select>
    <select id="countVo" resultType="java.lang.Long">
        <include refid="baseCountSql"/>
    </select>
</mapper>
