<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zclcs.platform.system.mapper.MinioFileMapper">

    <sql id="baseSql">
        select mf.id,
               mf.bucket_id,
               mb.bucket_name,
               mf.file_name,
               mf.original_file_name,
               mf.content_type,
               mf.file_path,
               mf.create_at,
               mf.update_at
        from system_minio_file mf
                 inner join system_minio_bucket mb on mf.bucket_id = mb.id
            ${ew.customSqlSegment}
    </sql>
    <select id="findPageVo" resultType="com.zclcs.platform.system.api.bean.vo.MinioFileVo">
        <include refid="baseSql"/>
    </select>
    <select id="findListVo" resultType="com.zclcs.platform.system.api.bean.vo.MinioFileVo">
        <include refid="baseSql"/>
    </select>
    <select id="findOneVo" resultType="com.zclcs.platform.system.api.bean.vo.MinioFileVo">
        <include refid="baseSql"/>
    </select>
</mapper>
